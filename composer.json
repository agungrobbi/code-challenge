{
    "name": "agungrobbi/code-challenge",
    "description": "Code challenge.",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "scripts": {
        "test": "phpunit tests --colors",
        "analyse": "phpstan analyse /"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {"App\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
